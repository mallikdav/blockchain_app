/**
 * Coosta Property Indexing Model definition.
 */
namespace org.acme.mynetwork

asset Property identified by propertyId {
  o String propertyId
  o String addresss
  o String city
  o String state
  o String country
  --> CoostaUser owner
}

participant CoostaUser identified by coostaUserId {
  o String coostaUserId
  o String firstName
  o String lastName
}

transaction Titlement {
  --> Property property
  --> CoostaUser newOwner
}